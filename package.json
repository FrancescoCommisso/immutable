{
  "engines": {
    "composer": "^0.20.7"
  },
  "name": "immutable-network",
  "version": "0.0.1",
  "description": "A permanent, unblockable, uncensorable social network",
  "scripts": {
    "prepublish": "mkdirp ./dist && composer archive create --sourceType dir --sourceName . -a ./dist/immutable-network.bna",
    "pretest": "npm run lint",
    "lint": "eslint .",
    "test": "nyc mocha -t 0 test/*.js && cucumber-js",
    "bna": "composer archive create -t dir -n . ",
    "startAndCreate": "./startFabric.sh && ./createPeerAdminCard.sh",
    "install": "composer network install --card PeerAdmin@hlfv1 --archiveFile immutable-network@0.0.1.bna",
    "start": "composer network start --networkName immutable-network --networkVersion 0.0.1 --networkAdmin admin --networkAdminEnrollSecret adminpw --card PeerAdmin@hlfv1 --file networkadmin.card",
    "playground": "composer-playground",
    "go": "npm run bna && npm run startAndCreate && npm run install && npm run start && npm run playground"
  },
  "keywords": [
    "composer",
    "composer-network"
  ],
  "author": "Francesco Commisso",
  "email": "FrancescoCommissoDev@gmail.com",
  "license": "Apache-2.0",
  "devDependencies": {
    "composer-admin": "^0.20.7",
    "composer-cli": "^0.20.7",
    "composer-client": "^0.20.7",
    "composer-common": "^0.20.7",
    "composer-connector-embedded": "^0.20.7",
    "composer-cucumber-steps": "^0.20.7",
    "chai": "latest",
    "chai-as-promised": "latest",
    "cucumber": "^2.2.0",
    "eslint": "latest",
    "nyc": "latest",
    "mkdirp": "latest",
    "mocha": "latest"
  }
}
